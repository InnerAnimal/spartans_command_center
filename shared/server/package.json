{
  "name": "meauxbility-org",
  "version": "1.0.0",
  "description": "Meauxbility - Transform Your Pain into Purpose",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step required'",
    "test": "echo 'No tests specified'",
    "deploy": "./deploy-all.sh",
    "deploy:quick": "npm install && npm start",
    "setup:env": "bash setup-api-environment.sh",
    "setup:google": "bash setup-google-workspace.sh",
    "optimize:cloud": "bash optimize-google-cloud.sh",
    "monitor": "bash deploy-monitor.sh",
    "health": "curl http://localhost:3000/api/health",
    "mcp:start": "node server.js --mcp",
    "all:setup": "npm run setup:env && npm run setup:google",
    "all:deploy": "./deploy-all.sh",
    "validate": "npm audit && npm run health"
  },
  "keywords": ["meauxbility", "nonprofit", "pain", "purpose", "healing"],
  "author": "Meauxbility Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "@supabase/supabase-js": "^2.38.4",
    "stripe": "^14.9.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}