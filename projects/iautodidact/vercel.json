{
  "version": 2,
  "name": "iautodidact-app",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "../../shared/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "../../shared/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "../../shared/server/server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "PROJECT_NAME": "iautodidact"
  }
}
